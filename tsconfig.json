{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "target": "ES5",
    "strict": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "baseUrl": "/home/ray/GitHub/webprojects/Practice",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@utils/*": ["src/utils/*"]
    },
    "plugins": [
      {
        "name": "@astro/ts-plugin"
      }
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
